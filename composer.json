{
  "name": "dpfaffenbauer/process-manager",
  "type": "pimcore-bundle",
  "license": "GPL-3.0-or-later",
  "description": "Process Manager helps you to see statuses for long running Processes",
  "keywords": [
    "pimcore",
    "pimcore-plugin"
  ],
  "homepage": "https://www.pfaffenbauer.at",
  "authors": [
    {
      "name": "Dominik Pfaffenbauer",
      "email": "dominik@pfaffenbauer.at",
      "homepage": "https://www.pfaffenbauer.at/",
      "role": "Developer"
    }
  ],
  "require": {
    "coreshop/resource-bundle": "^3.0",
    "pimcore/pimcore": "^6.7",
    "dragonmantank/cron-expression": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "ProcessManagerBundle\\": "src/ProcessManagerBundle"
    },
    "classmap": [
      "app/AppKernel.php"
    ]
  },
  "extra": {
    "pimcore": {
      "bundles": [
        "ProcessManagerBundle\\ProcessManagerBundle"
      ]
    },
    "branch-alias": {
      "dev-master": "3.0-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
